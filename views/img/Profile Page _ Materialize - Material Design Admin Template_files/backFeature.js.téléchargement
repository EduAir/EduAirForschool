var language;$(document).ready(function(){setlanguageMenu();var a=new language();language=top.language;a.loadLanguageJs(this,"about",load,language)});var jsonLanguage=[];var jsonIDLanguage=[];var setlanguageMenu=function(){var a=top.tlbs.templatesettings.langMenu;if(a!=null||a!=undefined){var c=a.split(",");for(i=0;i<c.length;i++){var b=c[i].split(":");jsonLanguage[b[1]]=b[0];jsonIDLanguage[b[0]]=b[1]}}else{}};var load=function(){$("#closeButton").xclick(function(){closeToolbar()});$("#cancelUnOrder").xclick(function(){$("#unOrderTips").hide();$(".confirm_alert").hide()});$(".unTipsClose").xclick(function(){$("#unOrderTips").hide();$(".confirm_alert").hide()});$(".unTipsClose,.unTipsClose_touch").xclick(function(){$("#unOrderTips").css("display","none");$("#changeLang").hide();$(".confirm_alert").hide()});$("#doneUnOrder").xclick(function(){startRequest()});$(".a-blue-btni").xclick(function(){close()});$("#successId").css("display","none");$("#failId").css("display","none");$("#languageButton").xclick(function(){$("#unOrderTips").hide();changeLanguageShow()});$("#french").xclick(function(){changeLanguage(jsonLanguage.FR,jsonIDLanguage["0"])});$("#english").xclick(function(){changeLanguage(jsonLanguage.EN,jsonIDLanguage["0"])});$("#succok").xclick(function(){$("#changeLangsucess").hide();$(".confirm_alert").hide()});$("#failok").xclick(function(){$("#changeLangfail").hide();$(".confirm_alert").hide()})};var op=0;var oderStatus=0;function changeLanguageShow(){$("#changeLang").show();$(".confirm_alert").show()}function changeLanguage(f,c){var b=top.tlbs.url+"tlbsserver/templatesettings/setlanguage?language="+f;var a=top.tlbs.tlbaurl+"/tlbaserver/traffic/setlanguage?language="+f;var e=top.tlbs.url+"tlbsserver/templatesettings/setlanguage?language="+c;var d=top.tlbs.tlbaurl+"/tlbaserver/traffic/setlanguage?language="+c;ajaxTlbsLanguageChange(b,a,e,d,false,false,true)}function ajaxTlbsLanguageChange(g,b,k,f,h,a,c){var j=false;if(!h){try{$.ajax({url:g,type:"GET",dataType:"jsonp",cache:false,async:false,timeout:10000,complete:function(l,e){if(l.status==200){j=true;$("#changeLang").hide();$(".confirm_alert").hide()}else{a=true;$("#changeLang").hide();$("#changeLangfail").show();$(".confirm_alert").show()}if(j&&!a&&!h){$("#changeLangsucess").show()}$(".confirm_alert").show()}})}catch(d){}}}function closeToolbar(){if(0==oderStatus){$("#unOrderTips").show();$(".confirm_alert").show()}$("#changeLang").hide()}function startRequest(){var a=document.createElement("iframe");a.style.display="none";a.onload=a.onreadystatechange=function(){if(!this.readyState||this.readyState==="loaded"||this.readyState==="complete"){$("#successId").css("display","block");$("#researchIntro").css("display","none");$(".intro-txt").css("display","none");oderStatus=1;$("#doneUnOrder").removeClass("disabled");$("#doneUnOrder").css("background-color","#0DA0ED");$("#unOrderTips").hide();$(".confirm_alert").hide()}else{$("#failId").css("display","block");$("#researchIntro").css("display","none");$(".intro-txt").css("display","none");$("#doneUnOrder").removeClass("disabled");$("#doneUnOrder").css("background-color","#0DA0ED");$("#unOrderTips").hide();$(".confirm_alert").hide()}};a.src=top.tlbs.tlbaurl+"/tlbagui/about_en/html/post.html";document.body.appendChild(a);if(top.tlbs.ball){top.tlbs.ball.hidden=true;top.tlbs.ball.mixedPkg=true;$(top.tlbs.ball.drag).css("display","none")}}function close(){top.tlbs.autohide.enable(false);if(oderStatus==1){top.tlbs.log("type=1&op=0");top.tlbs.animate.popup.hide(function(){top.tlbs.animate.content.hide(function(){$(top.document.body).children(".tlbs").css("display","none")})});if(top.tlbs.banner){top.tlbs.banner.elem.css("display","none");$(top.document.body).css("margin-top",0);top.tlbs.animate.resolveScroll.setMarginTop("0px")}if(top.tlbs.ball){top.tlbs.ball.hidden=true;top.tlbs.ball.mixedPkg=true;$("tlbs-flux",top.document).remove()}}else{if(oderStatus==0){$("#failId").css("display","none");$("#researchIntro").css("display","block");$(".intro-txt").css("display","block")}}};