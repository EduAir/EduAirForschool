function language(){var a;this.loadLanguageJs=function(d,e,f,g){var b;if(g==undefined||g==null){b=getUserLanguage(d)}else{b=g}if(g.length>2){b=g.replace(/\?[a-zA-z0-9=_&\?]{0,}/i,"")}var c=new Date();localePath="/tlbagui/locale/"+b+"/"+e+"_"+b+".js?vv=5555&d="+c.getMonth()+"_"+c.getYear();if(d.baseURI!=undefined){localePath=top.tlbs.tlbaurl+localePath}$.ajax({cache:true});$.getScript(localePath,function(h){}).done(function(){}).error(function(){}).then(function(h,j,i){if(f!=null||f!=undefined){f()}})}}var getUserLanguage=function(b){var a;try{a=top.tlbs.templatesettings.language}catch(c){a=getQueryString("language")}return a};var renderLanguage=function(a){$.each(a,function(b,c){if($("[lang-id='"+b+"']").is(":input")){$("[lang-id='"+b+"']").val(c)}else{$("[lang-id='"+b+"']").html(c)}})};